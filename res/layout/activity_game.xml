<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android1="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:layout_marginBottom="5dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginTop="5dp"
    android1:columnCount="1"
    android1:orientation="vertical"
    tools:context="${relativePackage}.${activityClass}" >

    <TextView
        android1:id="@+id/textView6"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignParentLeft="true"
        android:layout_marginTop="5dp"
        android1:text="@string/address"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android1:id="@+id/textView1"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@+id/textView6"
        android1:text="@string/date"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android1:id="@+id/textView2"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@+id/textView1"
        android1:text="@string/time"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android1:id="@+id/textView3"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@+id/textView2"
        android1:text="@string/gender"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android1:id="@+id/textView4"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@+id/textView3"
        android1:text="@string/age"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android1:id="@+id/textView5"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@+id/textView4"
        android1:text="@string/game_type"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android1:id="@+id/textView7"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android:layout_marginTop="25dp"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@+id/textView5"
        android1:text="@string/pitch"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android1:id="@+id/textView8"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@+id/textView7"
        android1:text="@string/skill_level"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android1:id="@+id/textView9"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android1:layout_alignParentLeft="true"
        android1:layout_below="@+id/textView8"
        android1:text="@string/max_number"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <EditText
        android1:id="@+id/editText1"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignParentRight="true"
        android1:layout_alignParentTop="true"
        android1:layout_toRightOf="@+id/textView5"
        android1:inputType="textPostalAddress" >

        <requestFocus />
    </EditText>

    <EditText
        android1:id="@+id/editText2"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/textView1"
        android1:layout_alignBottom="@+id/textView1"
        android1:layout_alignLeft="@+id/editText1"
        android1:layout_alignParentRight="true"
        android1:inputType="date" />

    <EditText
        android1:id="@+id/editText3"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/textView2"
        android1:layout_alignBottom="@+id/textView2"
        android1:layout_alignLeft="@+id/editText2"
        android1:layout_alignParentRight="true"
        android1:inputType="time" />

    <Button
        android1:id="@+id/button1"
        style="?android:attr/buttonStyleSmall"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/textView3"
        android1:layout_alignBottom="@+id/textView3"
        android1:layout_alignLeft="@+id/editText3"
        android1:text="Button" />

    <Button
        android1:id="@+id/button2"
        style="?android:attr/buttonStyleSmall"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/button1"
        android1:layout_alignBottom="@+id/button1"
        android1:layout_toRightOf="@+id/button1"
        android1:text="Button" />

    <Button
        android1:id="@+id/button3"
        style="?android:attr/buttonStyleSmall"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/button2"
        android1:layout_alignBottom="@+id/button2"
        android1:layout_toRightOf="@+id/button2"
        android1:text="Button" />

    <EditText
        android1:id="@+id/editText4"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/textView4"
        android1:layout_alignBottom="@+id/textView4"
        android1:layout_alignLeft="@+id/button1"
        android1:layout_alignRight="@+id/button1"
        android1:inputType="number" />

    <EditText
        android1:id="@+id/editText5"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/editText4"
        android1:layout_alignBottom="@+id/editText4"
        android1:layout_alignLeft="@+id/button3"
        android1:layout_alignRight="@+id/button3"
        android1:inputType="number" />

    <TextView
        android1:id="@+id/textView10"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/editText4"
        android1:layout_alignBottom="@+id/editText4"
        android1:layout_alignLeft="@+id/button2"
        android1:layout_alignRight="@+id/button2"
        android1:gravity="center"
        android1:text="@string/to"
        android1:textAppearance="?android:attr/textAppearanceMedium" />

    <Button
        android1:id="@+id/button4"
        style="?android:attr/buttonStyleSmall"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/textView5"
        android1:layout_alignBottom="@+id/textView5"
        android1:layout_alignLeft="@+id/editText4"
        android1:text="Button" />

    <Button
        android1:id="@+id/button5"
        style="?android:attr/buttonStyleSmall"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/button4"
        android1:layout_alignBottom="@+id/button4"
        android1:layout_toLeftOf="@+id/editText5"
        android1:text="Button" />

    <Button
        android1:id="@+id/button6"
        style="?android:attr/buttonStyleSmall"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/textView7"
        android1:layout_alignBottom="@+id/textView7"
        android1:layout_alignLeft="@+id/button4"
        android1:text="Button" />

    <Button
        android1:id="@+id/button7"
        style="?android:attr/buttonStyleSmall"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/button6"
        android1:layout_alignBottom="@+id/button6"
        android1:layout_toRightOf="@+id/button6"
        android1:text="Button" />

    <Button
        android1:id="@+id/button8"
        style="?android:attr/buttonStyleSmall"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignBaseline="@+id/button7"
        android1:layout_alignBottom="@+id/button7"
        android1:layout_alignLeft="@+id/editText5"
        android1:text="Button" />

    <SeekBar
        android1:id="@+id/seekBar1"
        android1:layout_width="match_parent"
        android1:layout_height="wrap_content"
        android1:layout_alignBottom="@+id/textView8"
        android1:layout_alignLeft="@+id/button6" />

    <EditText
        android1:id="@+id/editText6"
        android1:layout_width="wrap_content"
        android1:layout_height="wrap_content"
        android1:layout_alignLeft="@+id/button8"
        android1:layout_alignRight="@+id/button8"
        android1:layout_below="@+id/seekBar1"
        android1:inputType="number" />

</RelativeLayout>